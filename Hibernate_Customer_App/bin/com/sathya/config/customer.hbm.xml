<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping>
	<class name="com.satya.entity.Customer"  table="Customer_tab">
	<id name="customerId"  column="customer_id" length="12">
	<generator class="com.sathya.id.CustomerIdGenerator"/>
	</id>
	<property name="customerName"  column="Customer_name" length="12"/>
	<property name="city"  column="city" length="12">
	</property>
	</class>
	
	</hibernate-mapping>