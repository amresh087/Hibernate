package com.sathya.entity;

import java.util.Date;

import javax.persistence.DiscriminatorValue;
import javax.persistence.Entity;

@Entity
@DiscriminatorValue(value="card")

public class Cards extends Payments
{
private int cardNo;
private String cardType;
@Override
public String toString() {
	return "Cards [cardNo=" + cardNo + ", cardType=" + cardType + ", getCardNo()=" + getCardNo() + ", getCardType()="
			+ getCardType() + ", getTxId()=" + getTxId() + ", getAmount()=" + getAmount() + ", getDate()=" + getDate()
			+ ", toString()=" + super.toString() + ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + "]";
}
public Cards(int txId, double amount, Date date, int cardNo, String cardType) {
	super(txId, amount, date);
	this.cardNo = cardNo;
	this.cardType = cardType;
}
public Cards(int txId, double amount, Date date) {
	super(txId, amount, date);
	// TODO Auto-generated constructor stub
}
public Cards() {
	super();
	// TODO Auto-generated constructor stub
}

public int getCardNo() {
	return cardNo;
}
public void setCardNo(int cardNo) {
	this.cardNo = cardNo;
}
public String getCardType() {
	return cardType;
}
public void setCardType(String cardType) {
	this.cardType = cardType;
}


}
