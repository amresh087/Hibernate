<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping>
	<class name="com.sathya.entity.Employee"  table="emp">
	<id name="empNumber"  column="empno"/>
	
	<property name="empDeptno"  column="deptno"/>
	<property name="empSal"  column="salary"/>
	<property name="empName" column="empName"/>
	
	</class>
	
	</hibernate-mapping>